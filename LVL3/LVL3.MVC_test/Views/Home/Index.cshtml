@model IEnumerable<LVL3.Common.ViewModels.VehicleMakeViewModel>
@{
    ViewBag.Title = "Home Page";
}

<table class="table">

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.VehicleMakeId</td>
            <td>@item.Name</td>
            <td>@item.Abrv</td>
        </tr>
    }

</table>